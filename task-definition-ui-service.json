{
  "family": "metaflow-ui-service",
  "executionRoleArn": "arn:aws:iam::913330534208:role/oss-dev-ECSTaskExecutionRole-AVSF9XIE87Q4",
  "networkMode": "awsvpc",
  "memory": "1024",
  "cpu": "512",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "metaflow-ui-service",
      "image": "metaflow-ui-service",
      "command": [],
      "entryPoint": [],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/oss-dev-metaflow-ui-service",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "containerPort": 8083,
          "hostPort": 8083,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
